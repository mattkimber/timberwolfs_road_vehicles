item(FEAT_ROADVEHS, ford_model_t, 5121) {
    property {
        
    name:                           string(STR_NAME_FORD_MODEL_T);
    climates_available:             param_enable_vans  ? bitmask(CLIMATE_TEMPERATE, CLIMATE_ARCTIC, CLIMATE_TROPICAL) : NO_CLIMATE;
    introduction_date:              date(1912,1,01);
    model_life:                     28;

    vehicle_life:                   12;
    reliability_decay:              20;
    loading_speed:                  5;

    sprite_id:                      SPRITE_ID_NEW_ROADVEH; 
    speed:                          40 mph;
    misc_flags:                     bitmask(ROADVEH_FLAG_2CC);
    refit_cost:                     0; 

    running_cost_base:              RUNNING_COST_ROADVEH;
    weight:                         1 ton;     

    cargo_capacity:                 param_realistic_early_vehicle_capacity ? 1 : 4;
    cost_factor:                    param_realistic_early_vehicle_capacity ? 4 : 9;
    running_cost_factor:            param_realistic_early_vehicle_capacity ? 18 : 18;
    power:                          20;
    sound_effect:                   SOUND_BUS_START_PULL_AWAY;
        refittable_cargo_classes:       bitmask(CC_EXPRESS, CC_MAIL, CC_REFRIGERATED, CC_ARMOURED);
        non_refittable_cargo_classes:   bitmask(CC_PASSENGERS, CC_BULK);
        default_cargo_type:             GOOD;
    }

    graphics {
        additional_text: return string(STR_NAME_TEXT_FORD_MODEL_T);
        
    

        cargo_capacity: switch_capacity_ford_model_t;
        default: spriteset_ford_model_t;
    }
}

