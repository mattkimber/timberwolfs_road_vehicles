item(FEAT_ROADVEHS, mercedes_sprinter, 6061) {
    property {
        
    name:                           string(STR_NAME_MERCEDES_SPRINTER);
    climates_available:             param_enable_vans  ? bitmask(CLIMATE_TEMPERATE, CLIMATE_ARCTIC, CLIMATE_TROPICAL) : NO_CLIMATE;
    introduction_date:              date(2006,1,01);
    model_life:                     -1;

    vehicle_life:                   15;
    reliability_decay:              20;
    loading_speed:                  5;

    sprite_id:                      SPRITE_ID_NEW_ROADVEH; 
    speed:                          85 mph;
    misc_flags:                     bitmask(ROADVEH_FLAG_2CC);
    refit_cost:                     0; 

    running_cost_base:              RUNNING_COST_ROADVEH;
    weight:                         2 ton;     

    cargo_capacity:                 param_realistic_modern_vehicle_capacity ? 3 : 12;
    cost_factor:                    param_realistic_modern_vehicle_capacity ? 14 : 27;
    running_cost_factor:            param_realistic_modern_vehicle_capacity ? 70 : 70;
    power:                          160;
    sound_effect:                   SOUND_BUS_START_PULL_AWAY;
        refittable_cargo_classes:       bitmask(CC_EXPRESS, CC_MAIL, CC_REFRIGERATED, CC_ARMOURED);
        non_refittable_cargo_classes:   bitmask(CC_PASSENGERS, CC_BULK);
        default_cargo_type:             GOOD;
    }

    graphics {
        additional_text: return string(STR_NAME_TEXT_MERCEDES_SPRINTER);
        
    speed: (current_year >= 1965 && param_car_speed_limits) ? 56 : 69;

        cargo_capacity: switch_capacity_mercedes_sprinter;
        default: spriteset_mercedes_sprinter;
    }
}

