  VALU: param_realistic_modern_vehicle_capacity ? 7 : 10;
  return param_realistic_modern_vehicle_capacity ? 3 : 10;
}

item(FEAT_ROADVEHS, ford_transit_mk3, 5861) {
    property {
        
    name:                           string(STR_NAME_FORD_TRANSIT_MK3);
    climates_available:             param_enable_vans  ? bitmask(CLIMATE_TEMPERATE, CLIMATE_ARCTIC, CLIMATE_TROPICAL) : NO_CLIMATE;
    introduction_date:              date(1986,1,01);
    model_life:                     20;

    vehicle_life:                   14;
    reliability_decay:              20;
    loading_speed:                  5;

    sprite_id:                      SPRITE_ID_NEW_ROADVEH; 
    speed:                          80 mph;
    misc_flags:                     bitmask(ROADVEH_FLAG_2CC);
    refit_cost:                     0; 

    running_cost_base:              RUNNING_COST_ROADVEH;
    weight:                         1.5 ton;     

    cargo_capacity:                 param_realistic_modern_vehicle_capacity ? 3 : 10;
    cost_factor:                    param_realistic_modern_vehicle_capacity ? 12 : 23;
    running_cost_factor:            param_realistic_modern_vehicle_capacity ? 58 : 58;
    power:                          100;
    sound_effect:                   SOUND_BUS_START_PULL_AWAY;
        refittable_cargo_classes:       bitmask(CC_EXPRESS, CC_MAIL, CC_REFRIGERATED, CC_ARMOURED);
        non_refittable_cargo_classes:   bitmask(CC_PASSENGERS, CC_BULK);
        default_cargo_type:             GOOD;
    }

    graphics {
        additional_text: return string(STR_NAME_TEXT_FORD_TRANSIT_MK3);
        
    speed: (current_year >= 1965 && param_car_speed_limits) ? 56 : 65;

        cargo_capacity: switch_capacity_ford_transit_mk3;
        default: spriteset_ford_transit_mk3;
    }
}

