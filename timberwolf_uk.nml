{0: return stagecoach;1: return stagecoach;
    return CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}

switch (FEAT_ROADVEHS, SELF, switch_stagecoach_graphics, position_in_consist ) 
{
    0:  switch_horses_animation;1: return spriteset_stagecoach;
}

switch (FEAT_ROADVEHS, SELF, switch_stagecoach_capacity, position_in_consist ) 
{0: return param_realistic_early_vehicle_capacity ? 1 : 1;1: return param_realistic_early_vehicle_capacity ? 11 : 11;}

switch (FEAT_ROADVEHS, SELF, switch_stagecoach_length, position_in_consist ) 
{0: return 4;1: return 4;    
}



switch (FEAT_ROADVEHS, SELF, switch_stagecoach_visual_effect, position_in_consist ) 
{
    return VISUAL_EFFECT_DISABLE;    
}

item(FEAT_ROADVEHS, stagecoach, 741) {
    property {
        
    name:                           string(STR_NAME_STAGECOACH);
    climates_available:             param_enable_buses && param_enable_steam ? bitmask(CLIMATE_TEMPERATE, CLIMATE_ARCTIC, CLIMATE_TROPICAL) : NO_CLIMATE;
    
    introduction_date:              date(1800,1,01);
    model_life:                     100;

    vehicle_life:                   12;
    reliability_decay:              20;
    loading_speed:                  4;

    sprite_id:                      SPRITE_ID_NEW_ROADVEH; 
    speed:                          18 mph;
    misc_flags:                     bitmask(ROADVEH_FLAG_2CC);
    refit_cost:                     0; 

    running_cost_base:              RUNNING_COST_ROADVEH;
    weight:                         1 ton;     

    cargo_capacity:                 param_realistic_early_vehicle_capacity ? 6 : 6;
    cost_factor:                    param_realistic_early_vehicle_capacity ? 8 : 10;
    running_cost_factor:            param_realistic_early_vehicle_capacity ? 6 : 13;
    power:                          param_realistic_early_vehicle_capacity ? 5 : 15;
    sound_effect:                   SOUND_HORSE;
        refittable_cargo_classes:       bitmask(CC_PASSENGERS);
        non_refittable_cargo_classes:   bitmask(CC_BULK, CC_LIQUID, CC_PIECE_GOODS, CC_EXPRESS, CC_REFRIGERATED, CC_ARMOURED, CC_MAIL);
        default_cargo_type:             PASS;

    }

    graphics {
        additional_text: return string(STR_NAME_TEXT_STAGECOACH);
        
    speed: param_realistic_early_vehicle_capacity ? 9 : 15;

        visual_effect: switch_stagecoach_visual_effect;
        articulated_part: switch_stagecoach_articulation;
        cargo_capacity: switch_stagecoach_capacity;
        length: switch_stagecoach_length;
        purchase_cargo_capacity: spriteset_purchase_stagecoach;
        purchase: spriteset_purchase_stagecoach;
        default: switch_stagecoach_graphics;
  }
}



